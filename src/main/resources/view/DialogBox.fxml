  <children>
    <Label fx:id="dialog" minHeight="-Infinity" text="Label" textFill="#f2f2f2" wrapText="true">
         <HBox.margin>
            <Insets left="7.0" right="7.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="9.0" left="9.0" right="9.0" top="9.0" />
         </padding>
         <font>
            <Font name="Microsoft Sans Serif" size="14.0" />
         </font></Label>
    <ImageView fx:id="displayPicture" fitHeight="150.0" fitWidth="180.0" pickOnBounds="true" preserveRatio="true" />
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
  </padding>
</fx:root>
